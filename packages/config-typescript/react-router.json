{
    "extends": "./base.json",
    "compilerOptions": {
        "types": ["@react-router/node", "vite/client"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "moduleDetection": "force",
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    }
}
