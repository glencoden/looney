{
    "extends": "./base.json",
    "compilerOptions": {
        "types": ["@remix-run/node", "vite/client"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "moduleDetection": "force",
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["./app/*"]
        },
        "noEmit": true
    }
}
